{
  "name": "teams-generator",
  "version": "1.2.0",
  "private": true,
  "description": "Generate teams randomly based on rank. This is useful if you want to create lineups of balanced teams.",
  "type": "module",
  "engines": {
    "node": "24.x",
    "pnpm": "10.x"
  },
  "packageManager": "pnpm@10.29.1+sha512.48dae233635a645768a3028d19545cacc1688639eeb1f3734e42d6d6b971afbf22aa1ac9af52a173d9c3a20c15857cfa400f19994d79a2f626fcc73fccda9bbc",
  "scripts": {
    "clean": "nuxt cleanup && rm -rf node_modules .generated .nuxt",
    "dev": "nuxt dev",
    "start": "node .output/server/index.mjs",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "run-s nuxt:prepare db:generate",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "test": "pnpm docker:up && playwright test && pnpm docker:down",
    "test:ui": "pnpm docker:up && playwright test --ui && pnpm docker:down",
    "test:dev": "pnpm docker:up && pnpm test:db:reset && nuxt dev --dotenv .env.test",
    "test:db:reset": "dotenv -e .env.test -- zen migrate reset --force",
    "test:build": "nuxt build --dotenv .env.test",
    "test:preview": "nuxt preview --dotenv .env.test",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "nuxt:prepare": "nuxt prepare",
    "nuxt:build": "nuxt build",
    "db:deploy": "zen migrate deploy",
    "db:generate": "zen generate --lite",
    "db:migrate": "zen migrate dev",
    "playwright": "playwright",
    "dotenv": "dotenv"
  },
  "devDependencies": {
    "@iconify-json/lucide": "1.2.89",
    "@nuxt/eslint": "1.14.0",
    "@nuxt/image": "2.0.0",
    "@nuxt/test-utils": "4.0.0",
    "@pinia/colada-devtools": "0.4.2",
    "@pinia/nuxt": "0.11.3",
    "@playwright/test": "1.58.2",
    "@types/lodash-es": "4.17.12",
    "@types/node": "24.10.12",
    "@types/pg": "8.16.0",
    "@vite-pwa/assets-generator": "1.0.2",
    "@vite-pwa/nuxt": "1.1.1",
    "@vueuse/core": "14.2.0",
    "@vueuse/nuxt": "14.2.0",
    "@zenstackhq/cli": "3.3.3",
    "dotenv-cli": "11.0.0",
    "eslint": "10.0.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.5",
    "npm-run-all2": "8.0.4",
    "nuxt": "4.3.1",
    "pinia": "3.0.4",
    "sharp": "0.34.5",
    "sharp-ico": "0.1.5",
    "std-env": "3.10.0",
    "vue-tsc": "3.2.4"
  },
  "dependencies": {
    "@formwerk/core": "0.14.4",
    "@nuxt/ui": "4.4.0",
    "@pinia/colada": "0.21.3",
    "@pinia/colada-nuxt": "0.3.1",
    "@vueuse/components": "14.2.0",
    "@vueuse/router": "14.2.0",
    "@zenstackhq/orm": "3.3.3",
    "@zenstackhq/plugin-policy": "3.3.3",
    "@zenstackhq/server": "3.3.3",
    "lodash-es": "4.17.23",
    "nuxt-ui-formwerk": "0.1.19",
    "pg": "8.18.0",
    "pg-connection-string": "2.11.0",
    "typescript": "5.9.3",
    "vue-slicksort": "2.0.5",
    "zenstack-pinia-colada": "0.2.1",
    "zod": "4.3.6"
  },
  "pnpm": {
    "overrides": {
      "sharp": "0.34.5"
    }
  }
}
