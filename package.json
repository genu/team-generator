{
  "name": "teams-generator",
  "private": true,
  "type": "module",
  "engines": {
    "node": "22.x",
    "pnpm": "10.x"
  },
  "packageManager": "pnpm@10.15.0+sha512.486ebc259d3e999a4e8691ce03b5cac4a71cbeca39372a9b762cb500cfdf0873e2cb16abe3d951b1ee2cf012503f027b98b6584e4df22524e0c7450d9ec7aa7b",
  "scripts": {
    "clean": "nuxt cleanup && rm -rf node_modules .generated .nuxt",
    "dev": "nuxt dev",
    "start": "NODE_OPTIONS='--no-experimental-require-module' node .output/server/index.mjs",
    "generate": "nuxt generate",
    "preview": "NODE_OPTIONS='--no-experimental-require-module' nuxt preview",
    "postinstall": "run-s nuxt:prepare db:generate",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "test": "pnpm docker:up -d && playwright test && pnpm docker:down",
    "test:db": "pnpm docker:down && pnpm docker:up",
    "test:dev": "zenstack generate && dotenv -e .env.test -- prisma migrate dev && nuxt dev --dotenv .env.test",
    "test:ci": "zenstack generate && dotenv -e .env.test -- prisma migrate dev && nuxt build --dotenv .env.test && NODE_OPTIONS='--no-experimental-require-module' nuxt preview --dotenv .env.test",
    "test:db:reset": "dotenv -e .env.test -- prisma migrate reset --force",
    "prisma": "dotenv -e .env -- prisma",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "zenstack": "zenstack",
    "nuxt:prepare": "nuxt prepare",
    "nuxt:build": "dotenv -e .env -- prisma migrate deploy && nuxt build",
    "db:generate": "zenstack generate --output .generated/zenstack",
    "db:migrate": "dotenv -e .env -- prisma migrate",
    "playwright": "playwright",
    "playwright:ui": "playwright test --ui",
    "dotenv": "dotenv"
  },
  "devDependencies": {
    "@nuxt/eslint": "1.9.0",
    "@nuxt/test-utils": "3.19.2",
    "@pinia/nuxt": "0.11.2",
    "@playwright/test": "1.55.0",
    "@types/lodash-es": "4.17.12",
    "@types/msgpack-lite": "0.1.11",
    "@types/node": "24.3.0",
    "@types/pg": "8.15.5",
    "@vueuse/core": "13.8.0",
    "@vueuse/nuxt": "13.8.0",
    "@zenstackhq/cli": "3.0.0-alpha.33",
    "dotenv-cli": "10.0.0",
    "eslint": "9.34.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "npm-run-all2": "8.0.4",
    "nuxt": "4.0.3",
    "pinia": "3.0.3",
    "prisma": "6.15.0",
    "std-env": "3.9.0",
    "vue-tsc": "3.0.6"
  },
  "dependencies": {
    "@formwerk/core": "0.11.6",
    "@iconify-json/ph": "1.2.2",
    "@nuxt/ui": "3.3.2",
    "@pinia/colada": "^0.17.2",
    "@pinia/colada-nuxt": "0.2.2",
    "@vueuse/components": "13.8.0",
    "@vueuse/router": "13.8.0",
    "@zenstackhq/runtime": "3.0.0-alpha.33",
    "lodash-es": "4.17.21",
    "pg": "8.16.3",
    "pg-connection-string": "2.9.1",
    "typescript": "5.9.2",
    "vue-slicksort": "2.0.5",
    "zenstack-pinia-colada": "file:../../os/zenstack-pinia-colada",
    "zod": "4.1.4"
  }
}
