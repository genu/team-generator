{
  "name": "teams-generator",
  "version": "1.1.0",
  "private": true,
  "description": "Generate teams randomly based on rank. This is useful if you want to create lineups of balanced teams.",
  "type": "module",
  "engines": {
    "node": "24.x",
    "pnpm": "10.x"
  },
  "packageManager": "pnpm@10.24.0+sha512.01ff8ae71b4419903b65c60fb2dc9d34cf8bb6e06d03bde112ef38f7a34d6904c424ba66bea5cdcf12890230bf39f9580473140ed9c946fef328b6e5238a345a",
  "scripts": {
    "clean": "nuxt cleanup && rm -rf node_modules .generated .nuxt",
    "dev": "nuxt dev",
    "start": "node .output/server/index.mjs",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "run-s nuxt:prepare db:generate",
    "changelogen": "changelogen",
    "release": "changelogen --release",
    "release:push-tags": "git push --follow-tags",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "test": "pnpm docker:up -d && playwright test && pnpm docker:down",
    "test:db": "pnpm docker:down && pnpm docker:up",
    "test:dev": "dotenv -e .env.test -- zen migrate reset --force && nuxt dev --dotenv .env.test",
    "test:db:reset": "dotenv -e .env.test -- zen migrate reset --force",
    "test:server": "nuxt preview --dotenv .env.test",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "nuxt:prepare": "nuxt prepare",
    "nuxt:build": "nuxt build",
    "db:deploy": "zen migrate deploy",
    "db:generate": "zen generate --lite",
    "db:migrate": "zen migrate dev",
    "playwright": "playwright",
    "playwright:ui": "playwright test --ui",
    "dotenv": "dotenv"
  },
  "devDependencies": {
    "@iconify-json/lucide": "^1.2.78",
    "@nuxt/eslint": "1.11.0",
    "@nuxt/image": "2.0.0",
    "@nuxt/test-utils": "3.21.0",
    "@pinia/colada-devtools": "^0.1.9",
    "@pinia/nuxt": "0.11.3",
    "@playwright/test": "1.57.0",
    "@types/lodash-es": "4.17.12",
    "@types/node": "24.10.1",
    "@types/pg": "8.15.6",
    "@vite-pwa/assets-generator": "^1.0.2",
    "@vite-pwa/nuxt": "^1.1.0",
    "@vueuse/core": "14.1.0",
    "@vueuse/nuxt": "14.1.0",
    "@zenstackhq/cli": "3.0.0-beta.27",
    "changelogen": "^0.6.2",
    "dotenv-cli": "^11.0.0",
    "eslint": "9.39.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "npm-run-all2": "8.0.4",
    "nuxt": "4.2.1",
    "pinia": "3.0.4",
    "sharp": "^0.34.5",
    "sharp-ico": "^0.1.5",
    "std-env": "3.10.0",
    "vue-tsc": "3.1.5"
  },
  "dependencies": {
    "@formwerk/core": "0.14.3",
    "@nuxt/ui": "4.2.1",
    "@pinia/colada": "^0.18.0",
    "@pinia/colada-nuxt": "0.2.4",
    "@vueuse/components": "14.1.0",
    "@vueuse/router": "14.1.0",
    "@zenstackhq/orm": "3.0.0-beta.27",
    "@zenstackhq/plugin-policy": "3.0.0-beta.27",
    "@zenstackhq/server": "3.0.0-beta.27",
    "lodash-es": "4.17.21",
    "pg": "8.16.3",
    "pg-connection-string": "2.9.1",
    "typescript": "5.9.3",
    "vue-slicksort": "2.0.5",
    "zenstack-pinia-colada": "0.1.5",
    "zod": "4.1.13"
  }
}
