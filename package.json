{
  "name": "teams-generator",
  "private": true,
  "type": "module",
  "engines": {
    "node": "20.x",
    "pnpm": "8.x"
  },
  "scripts": {
    "build": "prisma generate && prisma migrate deploy && nuxt build",
    "dev": "prisma migrate dev && nuxt dev",
    "start": "node .output/server/index.mjs",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "prisma generate",
    "prisma": "prisma",
    "playwright": "playwright",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "test": "pnpm docker:up -d && playwright test && pnpm docker:down",
    "test:db": "pnpm docker:down && pnpm docker:up",
    "test:dev": "dotenv -e .env.test -- prisma migrate dev && nuxt dev --dotenv .env.test --port 3001",
    "playwright:ui": "playwright test --ui",
    "dotenv": "dotenv"
  },
  "prisma": {
    "schema": "server/prisma/schema.prisma"
  },
  "devDependencies": {
    "@nuxtjs/google-fonts": "3.1.3",
    "@playwright/test": "^1.40.1",
    "@types/lodash-es": "4.17.12",
    "@types/msgpack-lite": "^0.1.11",
    "@types/node": "^20.10.0",
    "@vueuse/core": "^10.6.1",
    "@vueuse/nuxt": "^10.6.1",
    "dotenv-cli": "^7.3.0",
    "nuxt": "3.10.2",
    "prisma": "^5.6.0",
    "vue-tsc": "^1.8.24"
  },
  "dependencies": {
    "@floatie/widget-nuxt": "^2.1.1",
    "@formkit/nuxt": "^1.3.2",
    "@formkit/themes": "^1.3.2",
    "@iconify-json/ph": "^1.1.8",
    "@nuxt/ui": "npm:@nuxt/ui-edge@latest",
    "@prisma/client": "^5.6.0",
    "@tanstack/vue-query": "^5.9.0",
    "@vueuse/components": "10.8.0",
    "@vueuse/router": "^10.6.1",
    "html2canvas": "^1.4.1",
    "lodash-es": "4.17.21",
    "nanoid": "^5.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2",
    "vue-slicksort": "^2.0.5"
  }
}