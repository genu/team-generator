{
  "name": "teams-generator",
  "version": "1.1.0",
  "private": true,
  "description": "Generate teams randomly based on rank. This is useful if you want to create lineups of balanced teams.",
  "type": "module",
  "engines": {
    "node": "24.x",
    "pnpm": "10.x"
  },
  "packageManager": "pnpm@10.27.0+sha512.72d699da16b1179c14ba9e64dc71c9a40988cbdc65c264cb0e489db7de917f20dcf4d64d8723625f2969ba52d4b7e2a1170682d9ac2a5dcaeaab732b7e16f04a",
  "scripts": {
    "clean": "nuxt cleanup && rm -rf node_modules .generated .nuxt",
    "dev": "nuxt dev",
    "start": "node .output/server/index.mjs",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "run-s nuxt:prepare db:generate",
    "changelogen": "changelogen",
    "release": "changelogen --release",
    "release:push-tags": "git push --follow-tags",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "test": "pnpm docker:up && playwright test && pnpm docker:down",
    "test:ui": "pnpm docker:up && playwright test --ui && pnpm docker:down",
    "test:dev": "pnpm docker:up && pnpm test:server:dev",
    "test:server": "dotenv -e .env.test -- zen migrate reset --force && nuxt preview --dotenv .env.test",
    "test:server:dev": "dotenv -e .env.test -- zen migrate reset --force && nuxt dev --dotenv .env.test",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "nuxt:prepare": "nuxt prepare",
    "nuxt:build": "nuxt build",
    "db:deploy": "zen migrate deploy",
    "db:generate": "zen generate --lite",
    "db:migrate": "zen migrate dev",
    "playwright": "playwright",
    "dotenv": "dotenv"
  },
  "devDependencies": {
    "@iconify-json/lucide": "^1.2.82",
    "@nuxt/eslint": "1.12.1",
    "@nuxt/image": "2.0.0",
    "@nuxt/test-utils": "3.23.0",
    "@pinia/colada-devtools": "^0.4.1",
    "@pinia/nuxt": "0.11.3",
    "@playwright/test": "1.57.0",
    "@types/lodash-es": "4.17.12",
    "@types/node": "24.10.4",
    "@types/pg": "8.16.0",
    "@vite-pwa/assets-generator": "^1.0.2",
    "@vite-pwa/nuxt": "^1.1.0",
    "@vueuse/core": "14.1.0",
    "@vueuse/nuxt": "14.1.0",
    "@zenstackhq/cli": "3.2.0",
    "changelogen": "^0.6.2",
    "dotenv-cli": "^11.0.0",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "npm-run-all2": "8.0.4",
    "nuxt": "4.2.2",
    "pinia": "3.0.4",
    "sharp": "^0.34.5",
    "sharp-ico": "^0.1.5",
    "std-env": "3.10.0",
    "vue-tsc": "3.2.2"
  },
  "dependencies": {
    "@formwerk/core": "0.14.4",
    "@nuxt/ui": "4.3.0",
    "@pinia/colada": "^0.21.0",
    "@pinia/colada-nuxt": "0.3.0",
    "@vueuse/components": "14.1.0",
    "@vueuse/router": "14.1.0",
    "@zenstackhq/orm": "3.2.0",
    "@zenstackhq/plugin-policy": "3.2.0",
    "@zenstackhq/server": "3.2.0",
    "lodash-es": "4.17.22",
    "nuxt-ui-formwerk": "^0.1.13",
    "pg": "8.16.3",
    "pg-connection-string": "2.9.1",
    "typescript": "5.9.3",
    "vue-slicksort": "2.0.5",
    "zenstack-pinia-colada": "0.1.13",
    "zod": "4.3.5"
  },
  "pnpm": {
    "overrides": {
      "sharp": "0.34.5"
    }
  }
}
